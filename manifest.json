{
    "name": "Block Extension",
    "description": "Keep track of who you blocked and why",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "client/background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "client/popup.html"
    }
}